CHIP GetQ
{

    IN a[16], b[16],zy,ny;
    OUT q[16];

    PARTS:

    //we first select if null or x value is selected

    Mux16b2to1(s=zy , x0[0..15]=a[0..15], x1[0..15]=b[0..15], y[0..15]=q1);

    //we then check if its complement is taken
    
    Xor16bw1b(a[0..15]=q1, b=ny, out[0..15]=q[0..15]);
}